	unzip texinfo gcc-multilib build-essential chrpath socat \
	cpio python3-pip \
	python3-pexpect xz-utils \
	debianutils iputils-ping python3-git python3-jinja2 libegl1-mesa libsdl1.2-dev xterm

source agl-init-build-env


